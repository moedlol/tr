    createButton("Disco", function()
        local lighting = game:GetService("Lighting")
        local fogEffect = lighting:FindFirstChild("FogEffect") or Instance.new("ColorCorrectionEffect", lighting)
        fogEffect.Name = "FogEffect"
        
        local colors = {
            Color3.fromRGB(255, 0, 0),
            Color3.fromRGB(255, 165, 0),
            Color3.fromRGB(255, 255, 0),
            Color3.fromRGB(0, 255, 0),
            Color3.fromRGB(0, 0, 255),
            Color3.fromRGB(255, 192, 203),
            Color3.fromRGB(128, 0, 128)
        }
        
        local running = true
        local index = 1
        local transitionTime = 2
        local startTime = os.clock()
        
        while running do
            local currentTime = os.clock() - startTime
            local progress = currentTime / transitionTime
            
            local currentColor = colors[index]
            local nextColor = colors[(index % #colors) + 1]
            
            fogEffect.TintColor = currentColor:Lerp(nextColor, progress)
            
            if progress >= 1 then
                index = index + 1
                if index > #colors then
                    index = 1
                end
                startTime = os.clock()
            end
            
            task.wait()
        end
    end)

    createButton("Disco fog", function()
        local discoFogEnabled = not discoFogEnabled
        local colors = {
            Color3.fromRGB(255, 0, 0),
            Color3.fromRGB(255, 165, 0),
            Color3.fromRGB(255, 255, 0),
            Color3.fromRGB(0, 255, 0),
            Color3.fromRGB(0, 0, 255),
            Color3.fromRGB(255, 105, 180),
            Color3.fromRGB(128, 0, 128)
        }
        
        game.Lighting.FogStart = 0
        game.Lighting.FogEnd = 100

        while discoFogEnabled do
            for _, color in ipairs(colors) do
                if not discoFogEnabled then 
                    game.Lighting.FogColor = Color3.fromRGB(200, 200, 200)
                    return 
                end
                game.Lighting.FogColor = color
                wait(0.3)
            end
        end
    end)

    createButton("Ball Rain", function()
        local RunService = game:GetService("RunService")
        
        local function spawnBall()
            local ball = Instance.new("Part")
            ball.Shape = Enum.PartType.Ball
            ball.Size = Vector3.new(50, 50, 50)
            ball.Position = Vector3.new(math.random(-100, 100), 100, math.random(-100, 100))
            ball.Material = Enum.Material.SmoothPlastic
            ball.Anchored = false
            ball.CanCollide = true
            ball.Parent = game.Workspace

            spawn(function()
                while ball.Parent do
                    ball.Color = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                    task.wait(0.1)
                end
            end)

            game:GetService("Debris"):AddItem(ball, 10)
        end

        local running = true
        spawn(function()
            while running do
                spawnBall()
                task.wait(0.2)
            end
        end)
    end)

