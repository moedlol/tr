                if v.Character and v.Character:FindFirstChild("Humanoid") then
                    v.Character.Humanoid.WalkSpeed = 1
                    v.Character.Humanoid.JumpPower = 1
                end
            end
            
            task.wait(0.1)
        end
    end)

