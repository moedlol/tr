    createButton("Shedletsky??", function()
        local ID = 84419218047002
        local Skybox = true
        local particle = true

        local function applyDecals(part)
            local faces = {"Front", "Back", "Top", "Bottom", "Left", "Right"}
            for _, face in ipairs(faces) do
                local decal = Instance.new("Decal")
                decal.Texture = "http://www.roblox.com/asset/?id=" .. ID
                decal.Face = face
                decal.Parent = part
            end
        end

        for _, obj in ipairs(game.Workspace:GetChildren()) do
            if obj:IsA("Part") then
                applyDecals(obj)
            elseif obj:IsA("Model") then
                for _, part in ipairs(obj:GetChildren()) do
                    if part:IsA("Part") then
                        applyDecals(part)
                    end
                end
            end
        end

        if Skybox then
            local sky = Instance.new("Sky")
            sky.Parent = game.Lighting
            local skyboxId = "http://www.roblox.com/asset/?id=172423468"
            sky.SkyboxBk = skyboxId
            sky.SkyboxDn = skyboxId
            sky.SkyboxFt = skyboxId
            sky.SkyboxLf = skyboxId
            sky.SkyboxRt = skyboxId
            sky.SkyboxUp = skyboxId
        end

        if particle then
            local function addParticle(part)
                local particleEmitter = Instance.new("ParticleEmitter")
                particleEmitter.Texture = "http://www.roblox.com/asset/?id=" .. ID
                particleEmitter.Rate = 200
                particleEmitter.Parent = part
            end

            for _, obj in ipairs(game.Workspace:GetChildren()) do
                if obj:IsA("Part") then
                    addParticle(obj)
                elseif obj:IsA("Model") then
                    for _, part in ipairs(obj:GetChildren()) do
                        if part:IsA("Part") then
                            addParticle(part)
                        end
                    end
                end
            end
        end
    end)

createButton("Chicken arms", function()
    local Chicken = game.Players.LocalPlayer.Name
    local character = game.Workspace[Chicken]
    
    -- Left arm chicken pose
    character.Torso["Left Shoulder"].C0 = CFrame.new(-1.5, 0.5, 0) * 
        CFrame.fromEulerAnglesXYZ(0, math.pi/2, 0) * 
        CFrame.fromEulerAnglesXYZ(math.pi/2, 0, 0) * 
        CFrame.fromEulerAnglesXYZ(0, -math.pi/2, 0)
    character.Torso["Left Shoulder"].C1 = CFrame.new(0, 0.5, 0)
    
    -- Right arm chicken pose
    character.Torso["Right Shoulder"].C0 = CFrame.new(1.5, 0.5, 0) * 
        CFrame.fromEulerAnglesXYZ(0, -math.pi/2, 0) * 
        CFrame.fromEulerAnglesXYZ(math.pi/2, 0, 0) * 
        CFrame.fromEulerAnglesXYZ(0, -math.pi/2, 0)
    character.Torso["Right Shoulder"].C1 = CFrame.new(0, 0.5, 0)
end)

createButton("Head shake", function()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local neck = character:WaitForChild("Torso"):WaitForChild("Neck")

    -- Side-to-side shaking
    for x = 1, math.huge, 0.2 do 
        task.wait() 
        neck.C0 = CFrame.new(math.sin(x) / 1, 1.5, 0) 
        neck.C1 = CFrame.new(0, 0, 0) 
    end 

    -- Spinning head effect
    for x = 1, math.huge, 0.1 do 
        task.wait() 
        neck.C0 = CFrame.new(0, 1.5, 0) * CFrame.fromAxisAngle(Vector3.new(0, 1, 0), x) 
        neck.C1 = CFrame.new(0, 0, 0) 
    end 

    -- Big head effect on all players
    for _, player in ipairs(game.Players:GetPlayers()) do
        if player.Character and player.Character:FindFirstChild("Head") then
            local headMesh = player.Character.Head:FindFirstChildOfClass("SpecialMesh")
            if headMesh then
                headMesh.Scale = Vector3.new(100, 100, 100)
                task.wait(0.5)
                headMesh.Scale = Vector3.new(1.25, 1.25, 1.25)
            end
        end
    end
end)

createButton("Floating pad", function()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local torso = character:WaitForChild("Torso")

    local pad = Instance.new("Part")
    pad.Parent = workspace
    pad.Name = "FloatingPad_" .. player.Name
    pad.Size = Vector3.new(8, 1.2, 8)
    pad.Anchored = true
    pad.Locked = true
    pad.BrickColor = BrickColor.new(104) -- Dark stone gray
    
    -- Apply custom texture
    local texture = Instance.new("Decal")
    texture.Parent = pad
    texture.Texture = "rbxassetid://126968470687373"
    texture.Face = "Top"
    
    -- Smooth cylinder shape
    local mesh = Instance.new("CylinderMesh")
    mesh.Scale = Vector3.new(1, 0.5, 1)
    mesh.Parent = pad

    -- Movement loop (with proper cleanup)
    local connection
    connection = game:GetService("RunService").Heartbeat:Connect(function()
        pad.CFrame = CFrame.new(torso.Position.X, torso.Position.Y - 4, torso.Position.Z)
    end)

    -- Cleanup when player dies or leaves
    player.CharacterRemoving:Connect(function()
        connection:Disconnect()
        pad:Destroy()
    end)
end)

