    createButton("Animated hint", function()
        local hint = Instance.new("Hint")
        hint.Parent = game.Workspace
        hint.Text = ""

        local message = "Hello, your game has been FUCKED by Team Yaaz.."
        for i = 1, #message do
            hint.Text = string.sub(message, 1, i)
            wait(0.1)
        end

        wait(2)

        for i = #message, 1, -1 do
            hint.Text = string.sub(message, 1, i - 1)
            wait(0.1)
        end

        hint:Destroy()
    end)

