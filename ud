    createButton("666", function()
        for _, v in pairs(game.Workspace:GetDescendants()) do
            if v:IsA("BasePart") then
                v.BrickColor = BrickColor.new("Really black")
                v.TopSurface = "Smooth"
                v.BottomSurface = "Smooth"

                local selection = Instance.new("SelectionBox", v)
                selection.Adornee = v
                selection.Color3 = Color3.new(1, 0, 0)

                local pointLight = Instance.new("PointLight", v)
                pointLight.Color = Color3.new(1, 0, 0)
                pointLight.Range = 15
                pointLight.Brightness = 5

                local fire = Instance.new("Fire", v)
                fire.Size = 19
                fire.Heat = 22
            end
        end

        local lighting = game:GetService("Lighting")
        lighting.TimeOfDay = "00:00:00"
        lighting.Brightness = 0
        lighting.ShadowColor = Color3.new(0, 0, 0)
        lighting.Ambient = Color3.new(1, 0, 0)
        lighting.FogEnd = 200
        lighting.FogColor = Color3.new(0, 0, 0)
    end)

    createButton("Rainbow 666", function()
        local function rainbowEffect(selectionBox)
            local hue = 0
            while true do
                hue = (hue + 0.01) % 1
                selectionBox.Color3 = Color3.fromHSV(hue, 1, 1)
                wait(0.01)
            end
        end

        for _, v in pairs(game.Workspace:GetDescendants()) do
            if v:IsA("BasePart") then
                v.BrickColor = BrickColor.new("Really black")
                v.TopSurface = "Smooth"
                v.BottomSurface = "Smooth"

                local selection = Instance.new("SelectionBox", v)
                selection.Adornee = v
                selection.Color3 = Color3.new(1, 0, 0)
                coroutine.wrap(rainbowEffect)(selection)

                local pointLight = Instance.new("PointLight", v)
                pointLight.Color = Color3.new(1, 0, 0)
                pointLight.Range = 15
                pointLight.Brightness = 5

                local fire = Instance.new("Fire", v)
                fire.Size = 19
                fire.Heat = 22
            end
        end

        local lighting = game:GetService("Lighting")
        lighting.TimeOfDay = "00:00:00"
        lighting.Brightness = 0
        lighting.ShadowColor = Color3.new(0, 0, 0)
        lighting.Ambient = Color3.new(1, 0, 0)
        lighting.FogEnd = 200
        lighting.FogColor = Color3.new(0, 0, 0)
    end)

