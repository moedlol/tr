createButton("Face all billboard", function()
    local textureId = "rbxassetid://111936421767453" -- Defined inside createButton function

    for _, player in pairs(game.Players:GetPlayers()) do
        if player.Character then
            local head = player.Character:FindFirstChild("Head")
            if head then
                -- Remove existing BillboardGui to prevent duplicates
                if head:FindFirstChild("FaceBillboard") then
                    head:FindFirstChild("FaceBillboard"):Destroy()
                end

                -- Create BillboardGui
                local billboard = Instance.new("BillboardGui")
                billboard.Name = "FaceBillboard"
                billboard.Adornee = head
                billboard.Size = UDim2.new(2.5, 0, 2.5, 0) -- Adjusted to fit face
                billboard.StudsOffset = Vector3.new(0, 0, 0.01) -- Slightly in front of face
                billboard.AlwaysOnTop = true
                billboard.LightInfluence = 0
                billboard.Parent = head

                -- Create ImageLabel for the face texture
                local imageLabel = Instance.new("ImageLabel")
                imageLabel.Size = UDim2.new(1, 0, 1, 0)
                imageLabel.BackgroundTransparency = 1
                imageLabel.Image = textureId
                imageLabel.Parent = billboard

                -- Align BillboardGui to the face
                billboard.StudsOffsetWorldSpace = head.CFrame.LookVector * 0.01
            end
        end
    end
end)

createButton("Nootaz Warning: Be ready for chaos.", function()
    -- Variables
    local pingudecal = "http://www.roblox.com/asset/?id=382332426"
    local didpingu = false

    if didpingu then
        warn("Already Ran! Did not execute the script!")
        return
    end

    didpingu = true

    -- Play Sound (Music First, No Chaos Yet)
    local nooties = Instance.new("Sound", workspace)
    nooties.SoundId = "rbxassetid://82174917569958" -- Corrected sound ID format
    nooties.Volume = 1
    nooties.PlaybackSpeed = 0.102 -- Set sound speed
    nooties.Looped = true
    nooties.Parent = workspace
    nooties:Play()

    -- Wait Before Chaos Starts
    task.wait(10) -- 10 seconds of peaceful music, then chaos begins!

    -- INTENSE CHAOS STARTS HERE
    -- Set Skybox to Pingu Everywhere
    local Sky = Instance.new("Sky", game.Lighting)
    Sky.SkyboxBk = pingudecal
    Sky.SkyboxDn = pingudecal
    Sky.SkyboxFt = pingudecal
    Sky.SkyboxLf = pingudecal
    Sky.SkyboxRt = pingudecal
    Sky.SkyboxUp = pingudecal

    -- Add Pingu Decals to EVERY Part!
    local function addDecalsToPart(part)
        local faces = {"Top", "Bottom", "Left", "Right", "Front", "Back"}
        for _, face in pairs(faces) do
            local decal = Instance.new("Decal", part)
            decal.Face = Enum.NormalId[face]
            decal.Texture = pingudecal
        end
    end

    -- Explosions and Decals on EVERY PART!
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("Part") or obj:IsA("MeshPart") then
            -- Add Pingu Decals
            addDecalsToPart(obj)

            -- Add Particle Effect for Extra Chaos
            local VTest = Instance.new("ParticleEmitter", obj)
            VTest.Texture = pingudecal
            VTest.Speed = NumberRange.new(500) -- Faster effect!
            VTest.Rate = 2000 -- More particles!
            VTest.Lifetime = NumberRange.new(3) -- Short-lived spam!
            VTest.Size = NumberSequence.new(10, 100) -- Big particles!

            -- Spam Explosions
            coroutine.wrap(function()
                while true do
                    task.wait(0.1) -- Spam explosions every 0.1s!
                    local Explosion = Instance.new("Explosion", obj)
                    Explosion.BlastRadius = 50000 -- HUGE destruction!
                    Explosion.BlastPressure = 1000000 -- Super strong explosions!
                end
            end)()
        end
    end
end)

createButton("Toadroast", function()
    math.randomseed(tick() % 1 * 1e6)

    -- Sky Effect
    local sky = coroutine.create(function()
        while task.wait(0.3) do
            local s = Instance.new("Sky", game.Lighting)
            s.SkyboxBk, s.SkyboxDn, s.SkyboxFt, s.SkyboxLf, s.SkyboxRt, s.SkyboxUp = 
                "rbxassetid://201208408", "rbxassetid://201208408", "rbxassetid://201208408", 
                "rbxassetid://201208408", "rbxassetid://201208408", "rbxassetid://201208408"
            s.CelestialBodiesShown = false
        end
    end)

    -- Delete all models every 0.3s
    local del = coroutine.create(function()
        while task.wait(0.3) do
            for _, v in pairs(workspace:GetChildren()) do
                if v:IsA("Model") then
                    v:Destroy()
                end
            end
        end
    end)

    -- Sound IDs for random noises
    local noises = {'rbxassetid://230287740', 'rbxassetid://271787597', 'rbxassetid://153752123', 'rbxassetid://271787503'}

    -- Background Sound Loop (Raining Men Bypassed Audio)
    local sound = coroutine.create(function()
        while task.wait(0.2) do
            for i = 1, 3 do  -- Reduced to 3 overlapping sounds
                local extraSound = Instance.new("Sound", workspace)
                extraSound.SoundId = "rbxassetid://141509625"
                extraSound.Name = "RAINING MEN"
                extraSound.Volume = 10
                extraSound.Looped = false
                extraSound.Pitch = 0.2
                extraSound:Play()
                extraSound.Ended:Connect(function()
                    extraSound:Destroy()
                end)
            end
        end
    end)

    -- Repeating Message
    local msg = coroutine.create(function()
        while task.wait(0.4) do
            local message = Instance.new("Message", workspace)
            message.Text = "ITS THE FUCKING TOADROAST DAY OF YAAZ ðŸ˜±"
            task.wait(0.4)
            message:Destroy()
        end
    end)

    -- Faster Rain with better ground deletion
    local rain = coroutine.create(function()
        while task.wait(0.2) do
            for i = 1, 5 do  -- Reduced to 5 toads at a time
                local part = Instance.new("Part", workspace)
                part.Name = "Meshes/toad (3)"
                part.CanCollide = false
                part.Anchored = false
                part.Size = Vector3.new(440, 530, 380)
                part.Position = Vector3.new(math.random(-5000, 5000), math.random(100, 3000), math.random(-5000, 5000))

                local mesh = Instance.new("SpecialMesh", part)
                mesh.MeshType = "FileMesh"
                mesh.MeshId = "rbxassetid://7234998844"
                mesh.TextureId = "rbxassetid://1009824086"

                -- Play Loud Random Noise
                for i = 1, 3 do  -- Reduced to 3 overlapping noise sounds
                    local sound = Instance.new("Sound", part)
                    sound.SoundId = noises[math.random(1, #noises)]
                    sound.Volume = 10
                    sound:Play()
                    sound.Ended:Connect(function()
                        sound:Destroy()
                    end)
                end

                -- Improved Ground Deletion
                local deletePart = function()
                    if part and part.Parent then
                        part:Destroy()
                    end
                end

                -- Touched event for quick deletion
                part.Touched:Connect(function(hit)
                    if hit:IsA("BasePart") and hit.Position.Y < 12 then
                        deletePart()
                    end
                end)

                -- Timer-based deletion (Backup method)
                game:GetService("Debris"):AddItem(part, 5)  -- Auto delete in 5 seconds
            end
        end
    end)

    -- Start Coroutines
    coroutine.resume(sky)
    coroutine.resume(del)
    coroutine.resume(sound)
    coroutine.resume(msg)
    coroutine.resume(rain)

    -- Background Music (Max Volume)
    local music = Instance.new("Sound", workspace)
    music.Name = "BackgroundMusic"
    music.SoundId = "rbxassetid://97797838120232"
    music.Volume = 99999999999999999999999999999999999999999999999999
    music.Looped = true
    music.Pitch = 0.2
    music:Play()

    -- Warning Message
    local hint = Instance.new("Hint", workspace)
    hint.Text = "Imagine getting toadroast -YaazDaQuitos"
    task.wait(10)
    hint:Destroy()
end)

    createButton("IT'S HOTDOG DAY!!", function()
        local player = game.Players.LocalPlayer
        local character = player.Character
        if character then
            for _, part in pairs(game.Workspace:GetDescendants()) do
                if part:IsA("BasePart") or part:IsA("Decal") then
                    part.Transparency = 1
                end
            end

            for _, part in pairs(character:GetChildren()) do
                if part:IsA("BasePart") or part:IsA("Decal") then
                    part.Transparency = 1
                end
            end

            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = 0
                humanoid.JumpPower = 0
            end
        end

        local hint = Instance.new("Hint")
        hint.Text = "HOT DOGGED BY YAAZ? HOW LOSER!"
        hint.Parent = game.Workspace

        for _, sound in pairs(game.Workspace:GetDescendants()) do
            if sound:IsA("Sound") then
                sound:Stop()
            end
        end

        local hotDogSong = Instance.new("Sound")
        hotDogSong.SoundId = "rbxassetid://121252384962048"
        hotDogSong.Looped = true
        hotDogSong.Volume = 999999
        hotDogSong.Parent = game.Workspace
        hotDogSong:Play()

        spawn(function()
            while true do
                for i = 1, 6 do
                    local hotdog = Instance.new("Part", workspace)
                    hotdog.Name = "HotDog"
                    hotdog.Size = Vector3.new(1, 1, 1)
                    hotdog.Position = Vector3.new(
                        math.random(-250, 250),
                        math.random(80, 120),
                        math.random(-250, 250)
                    )
                    hotdog.CanCollide = false
                    hotdog.Anchored = false

                    local mesh = Instance.new("SpecialMesh", hotdog)
                    mesh.MeshType = "FileMesh"
                    mesh.MeshId = "rbxassetid://6318954843"
                    mesh.TextureId = "rbxassetid://6318954910"
                    mesh.Scale = Vector3.new(7, 7, 7)

                    hotdog.Velocity = Vector3.new(0, -50, 0)

                    spawn(function()
                        local startTime = tick() 
                        local lastVelocity = hotdog.AssemblyLinearVelocity.Magnitude
                        while hotdog and hotdog.Parent do
                            wait(0.5)

                            local currentVelocity = hotdog.AssemblyLinearVelocity.Magnitude
                            local isOnGround = hotdog.Position.Y < 5 

                            if (currentVelocity < 0.2 and lastVelocity < 0.2) or isOnGround then
                                wait(1) 
                                if hotdog and hotdog.Parent then
                                    hotdog:Destroy()
                                end
                                break
                            end

                            if tick() - startTime > 10 then 
                                if hotdog and hotdog.Parent then
                                    hotdog:Destroy()
                                end
                                break
                            end

                            lastVelocity = currentVelocity
                        end
                    end)
                end
                wait(0.5)
            end
        end)

        local skyboxCoroutine = coroutine.create(function()
            while wait(0.3) do
                for _, sky in pairs(game.Lighting:GetChildren()) do
                    if sky:IsA("Sky") then
                        sky:Destroy()
                    end
                end

                local skybox = Instance.new("Sky", game.Lighting)
                skybox.SkyboxBk = "rbxassetid://79236980509669"
                skybox.SkyboxDn = "rbxassetid://79236980509669"
                skybox.SkyboxFt = "rbxassetid://79236980509669"
                skybox.SkyboxLf = "rbxassetid://79236980509669"
                skybox.SkyboxRt = "rbxassetid://79236980509669"
                skybox.SkyboxUp = "rbxassetid://79236980509669"
                skybox.CelestialBodiesShown = false
            end
        end)

        coroutine.resume(skyboxCoroutine)

        local msg = coroutine.create(function()
            while wait(0.4) do
                local message = Instance.new("Message", workspace)
                message.Text = "GOT HOTDOGGED BY YAAZ AKA QUITOS LOOOOOLLL"
                wait(0.4)
                message:Destroy()
            end
        end)

        coroutine.resume(msg)
    end)

    createButton("Scary lol", function()
        local sound = Instance.new("Sound", game.Workspace)
        sound.SoundId = "rbxassetid://209322206"
        sound.Volume = 10
        sound.Looped = false
        sound:Play()

        for _, obj in pairs(game.Workspace:GetDescendants()) do
            if obj:IsA("BasePart") then
                obj.Color = math.random(1, 2) == 1 and Color3.new(0, 0, 0) or Color3.new(1, 0, 0)
                obj.Material = Enum.Material.Slate
            end
        end

        local lighting = game:GetService("Lighting")
        lighting.FogStart = 0
        lighting.FogEnd = 50
        lighting.FogColor = Color3.new(0, 0, 0)
        lighting.Ambient = Color3.new(0.3, 0, 0)
        lighting.OutdoorAmbient = Color3.new(0.3, 0, 0)
        lighting.Brightness = 0.2

        for i = 1, 5 do
            local msg = Instance.new("Message", game.Workspace)
            msg.Text = "YOU CAN'T ESCAPE..."
            game:GetService("Debris"):AddItem(msg, 2)
        end

        wait(1)

        local jumpscareGui = Instance.new("ScreenGui", game.Players.LocalPlayer.PlayerGui)
        local jumpscareImg = Instance.new("ImageLabel", jumpscareGui)
        jumpscareImg.Size = UDim2.new(1, 0, 1, 0)
        jumpscareImg.Image = "rbxassetid://135547930230734"
        jumpscareImg.BackgroundTransparency = 1
        jumpscareImg.ZIndex = 9999

        local jumpscareSound = Instance.new("Sound", jumpscareGui)
        jumpscareSound.SoundId = "rbxassetid://7991927928"
        jumpscareSound.Volume = 10
        jumpscareSound.Looped = false
        jumpscareSound:Play()
        wait(5)
        jumpscareGui:Destroy()
    end)
    

    createButton("Plane", function()
        local player = game.Players.LocalPlayer
        local char = player.Character or player.CharacterAdded:Wait()

        if not char:FindFirstChild("HumanoidRootPart") then return end

        for _, v in pairs(char:GetChildren()) do
            if v:IsA("BasePart") then
                v.Transparency = 1
            elseif v:IsA("Accessory") or v:IsA("Clothing") then
                v.Parent = nil
            end
        end

        local planeModel = Instance.new("Model", workspace)
        planeModel.Name = "PlayerPlane"

        local body = Instance.new("Part", planeModel)
        body.Size = Vector3.new(8, 2, 14)
        body.Position = char.HumanoidRootPart.Position
        body.Shape = Enum.PartType.Cylinder
        body.Orientation = Vector3.new(0, 90, 0)
        body.Color = Color3.fromRGB(255, 255, 255)
        body.Material = Enum.Material.SmoothPlastic
        body.Anchored = false
        body.CanCollide = false

        local nose = Instance.new("Part", planeModel)
        nose.Size = Vector3.new(4, 2, 4)
        nose.Shape = Enum.PartType.Ball
        nose.Position = body.Position + Vector3.new(0, 0, 7)
        nose.Color = Color3.fromRGB(255, 255, 255)
        nose.Material = Enum.Material.SmoothPlastic
        nose.Anchored = false
        nose.CanCollide = false

        local cockpit = Instance.new("Part", planeModel)
        cockpit.Size = Vector3.new(3, 1.5, 2)
        cockpit.Position = body.Position + Vector3.new(0, 1, 5)
        cockpit.Material = Enum.Material.Glass
        cockpit.Color = Color3.fromRGB(0, 150, 255)
        cockpit.Anchored = false
        cockpit.CanCollide = false

        local leftWing = Instance.new("Part", planeModel)
        leftWing.Size = Vector3.new(10, 0.5, 3)
        leftWing.Position = body.Position + Vector3.new(-6, 0, 0)
        leftWing.Color = Color3.fromRGB(255, 255, 255)
        leftWing.Anchored = false
        leftWing.CanCollide = false

        local rightWing = leftWing:Clone()
        rightWing.Position = body.Position + Vector3.new(6, 0, 0)
        rightWing.Parent = planeModel

        local tailWing = Instance.new("Part", planeModel)
        tailWing.Size = Vector3.new(4, 0.5, 2)
        tailWing.Position = body.Position + Vector3.new(0, 1, -7)
        tailWing.Color = Color3.fromRGB(255, 255, 255)
        tailWing.Anchored = false
        tailWing.CanCollide = false

        local engineLeft = Instance.new("Part", planeModel)
        engineLeft.Size = Vector3.new(1.5, 1.5, 3)
        engineLeft.Shape = Enum.PartType.Cylinder
        engineLeft.Orientation = Enum.PartType.Cylinder
        engineLeft.Position = leftWing.Position + Vector3.new(0, -1, 0)
        engineLeft.Color = Color3.fromRGB(100, 100, 100)
        engineLeft.Anchored = false
        engineLeft.CanCollide = false

        local engineRight = engineLeft:Clone()
        engineRight.Position = rightWing.Position + Vector3.new(0, -1, 0)
        engineRight.Parent = planeModel

        local function weldParts(part0, part1)
            local weld = Instance.new("Weld", part0)
            weld.Part0 = part0
            weld.Part1 = part1
            weld.C0 = part0.CFrame:inverse() * part1.CFrame
        end

        weldParts(body, nose)
        weldParts(body, cockpit)
        weldParts(body, leftWing)
        weldParts(body, rightWing)
        weldParts(body, tailWing)
        weldParts(body, engineLeft)
        weldParts(body, engineRight)

        local weld = Instance.new("Weld", body)
        weld.Part0 = char.HumanoidRootPart
        weld.Part1 = body
        weld.C0 = CFrame.new(0, 0, 0)

        local bodyVelocity = Instance.new("BodyVelocity", char.HumanoidRootPart)
        bodyVelocity.Velocity = Vector3.new(0, 0, 0)
        bodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)

        local function NoClip()
            for _, v in pairs(char:GetDescendants()) do
                if v:IsA("BasePart") then
                    v.CanCollide = false
                end
            end
        end

        local trail = Instance.new("Trail", body)
        local attachment0 = Instance.new("Attachment", body)
        local attachment1 = Instance.new("Attachment", body)
        attachment0.Position = Vector3.new(0, 0, -6)
        attachment1.Position = Vector3.new(0, 0, 6)
        trail.Attachment0 = attachment0
        trail.Attachment1 = attachment1
        trail.Color = ColorSequence.new(Color3.fromRGB(255, 255, 255), Color3.fromRGB(0, 150, 255))
        trail.Lifetime = 0.5
        trail.Enabled = true

        local flying = true
        local speed = 50
        local up = false
        local down = false
        local UIS = game:GetService("UserInputService")

        UIS.InputBegan:Connect(function(input)
            if input.KeyCode == Enum.KeyCode.W then up = true end
            if input.KeyCode == Enum.KeyCode.S then down = true end
        end)

        UIS.InputEnded:Connect(function(input)
            if input.KeyCode == Enum.KeyCode.W then up = false end
            if input.KeyCode == Enum.KeyCode.S then down = false end
        end)

        local function GetJoystickInput()
            return player:GetMouse().Hit.p - char.HumanoidRootPart.Position
        end

        game:GetService("RunService").RenderStepped:Connect(function()
            if flying then
                NoClip()
                local direction = char.HumanoidRootPart.CFrame.LookVector
                local yVelocity = 0

                if UIS.TouchEnabled then
                    local moveDir = GetJoystickInput()
                    if moveDir.Y > 0.5 then
                        yVelocity = 30
                    elseif moveDir.Y < -0.5 then
                        yVelocity = -30
                    end
                else
                    if up then yVelocity = 30 end
                    if down then yVelocity = -30 end
                end

                bodyVelocity.Velocity = direction * speed + Vector3.new(0, yVelocity, 0)
            end
        end)

        char.Humanoid.Died:Connect(function()
            flying = false
            planeModel:Destroy()
            for _, v in pairs(char:GetChildren()) do
                if v:IsA("BasePart") then v.Transparency = 0 end
            end
        end)
    end)

    createButton("Nachos", function()
        local SKYBOX_ID = "rbxassetid://116355300471902"
        local MESH_ID = "rbxassetid://13049164651"
        local TEXTURE_ID = "rbxassetid://13049164734"
        local SOUND_ID = "rbxassetid://1845230355"

        local function createSkybox()
            local sky = Instance.new("Sky", game.Lighting)
            sky.SkyboxBk = SKYBOX_ID
            sky.SkyboxDn = SKYBOX_ID
            sky.SkyboxFt = SKYBOX_ID
            sky.SkyboxLf = SKYBOX_ID
            sky.SkyboxRt = SKYBOX_ID
            sky.SkyboxUp = SKYBOX_ID
            sky.CelestialBodiesShown = false
        end

        local function destroyModels()
            for _, model in pairs(workspace:GetChildren()) do
                if model:IsA("Model") then
                    model:Destroy()
                end
            end
        end

        local function playNachosSound()
            local sound = Instance.new("Sound", workspace)
            sound.SoundId = SOUND_ID
            sound.Name = "NACHOS"
            sound.Volume = 0.5
            sound.Looped = true
            sound:Play()
            return sound
        end

        local function spawnNachos()
            local part = Instance.new("Part", workspace)
            part.Name = "Nachos"
            part.CanCollide = false
            part.Anchored = false
            part.Size = Vector3.new(5, 5, 5)
            part.Position = Vector3.new(math.random(-1000, 1000), 200, math.random(-1000, 1000))

            local mesh = Instance.new("SpecialMesh", part)
            mesh.MeshId = MESH_ID
            mesh.TextureId = TEXTURE_ID
            mesh.MeshType = "FileMesh"
            mesh.Scale = Vector3.new(20, 20, 20)
        end

        local function displayNachosMessage()
            local msg = Instance.new("Message", workspace)
            msg.Text = "Vangel how did you got nachoed by Yaaz you black nigger"
            wait(0.4)
            msg:Destroy()
        end

        local function skyboxLoop()
            while wait(0.3) do
                createSkybox()
            end
        end

        local function destroyLoop()
            while wait(0.3) do
                destroyModels()
            end
        end

        local function soundLoop()
            local sound = playNachosSound()
            while wait(1) do
                if not workspace:FindFirstChild("NACHOS") then
                    sound = playNachosSound()
                end
            end
        end

        local function messageLoop()
            while wait(0.4) do
                displayNachosMessage()
            end
        end

        local function nachosRainLoop()
            while wait(1 / 30) do
                spawnNachos()
            end
        end

        coroutine.wrap(skyboxLoop)()
        coroutine.wrap(destroyLoop)()
        coroutine.wrap(soundLoop)()
        coroutine.wrap(messageLoop)()
        coroutine.wrap(nachosRainLoop)()

        local bgSound = Instance.new("Sound")
        bgSound.SoundId = SOUND_ID
        bgSound.Volume = 0.99
        bgSound.Looped = true
        bgSound.Parent = workspace
        bgSound:Play()

        local hint = Instance.new("Hint", workspace)
        hint.Text = "Um, nachos"
        wait(10)
        hint:Destroy()
    end)

