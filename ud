    createButton("Map rotate", function()
        local baseplate = workspace:FindFirstChild("Baseplate")
        if baseplate and baseplate:IsA("BasePart") then
            local tiltSpeed = 1
            local maxTilt = 45

            while true do
                for angle = 0, maxTilt, tiltSpeed do
                    baseplate.CFrame = baseplate.CFrame * CFrame.Angles(math.rad(tiltSpeed), 0, 0)
                    task.wait(0.1)
                end
                for angle = maxTilt, 0, -tiltSpeed do
                    baseplate.CFrame = baseplate.CFrame * CFrame.Angles(math.rad(-tiltSpeed), 0, 0)
                    task.wait(0.1)
                end
            end
        end
    end)

createButton("Head shake", function()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local head = character:FindFirstChild("Head")

    if not head then return end

    local runService = game:GetService("RunService")

    -- Create a Motor6D for smooth head rotation
    local neck = head:FindFirstChild("Neck") or Instance.new("Motor6D")
    neck.Part0 = character:FindFirstChild("UpperTorso") or character:FindFirstChild("Torso")
    neck.Part1 = head
    neck.Parent = head
    neck.Name = "Neck"

    -- Store the original C0 for reference
    local originalC0 = neck.C0

    -- Head shake animation
    task.spawn(function()
        while true do
            local angle = math.sin(tick() * 10) * 0.3 -- Left-right oscillation
            neck.C0 = originalC0 * CFrame.Angles(0, angle, 0)
            runService.RenderStepped:Wait()
        end
    end)
end)

createButton("Morph Duck", function()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    local humanoid = character:FindFirstChild("Humanoid")

    if not humanoidRootPart or not humanoid then return end

    -- Hide all character parts instead of destroying them (prevents reset)
    for _, v in pairs(character:GetChildren()) do
        if v:IsA("BasePart") or v:IsA("MeshPart") then
            v.Transparency = 1 -- Make body invisible
            v.CanCollide = false
        elseif v:IsA("Accessory") or v:IsA("Clothing") or v:IsA("Decal") then
            v:Destroy() -- Remove accessories and clothes
        end
    end

    -- Hide the face
    local head = character:FindFirstChild("Head")
    if head then
        local face = head:FindFirstChild("face")
        if face then
            face:Destroy()
        end
    end

    -- Create a new part for the duck morph
    local duckBody = Instance.new("Part")
    duckBody.Size = Vector3.new(5, 5, 5)
    duckBody.Name = "DuckBody"
    duckBody.Parent = character
    duckBody.Position = humanoidRootPart.Position
    duckBody.Anchored = false
    duckBody.CanCollide = false
    duckBody.Massless = true

    -- Add the duck mesh
    local mesh = Instance.new("SpecialMesh", duckBody)
    mesh.MeshId = "rbxassetid://17399705213" -- Duck Mesh
    mesh.TextureId = "rbxassetid://17399705259" -- Correct Duck Texture
    mesh.Scale = Vector3.new(5, 5, 5)

    -- ðŸ”¥ FIX: Adjust Offset to Center the Model
    mesh.Offset = Vector3.new(0, -2.5, 0) -- Move down slightly to align better

    -- ðŸ”¥ FIX: Rotate the Duck to Face Forward
    duckBody.Orientation = Vector3.new(0, 103, 0) -- Adjust rotation so it faces forward

    -- Weld the duck to the HumanoidRootPart (so it follows the player)
    local weld = Instance.new("WeldConstraint", duckBody)
    weld.Part0 = duckBody
    weld.Part1 = humanoidRootPart

    -- Set WalkSpeed to 100
    humanoid.WalkSpeed = 100

    -- Add "Duck" BillboardGUI above the player
    local billboard = Instance.new("BillboardGui", duckBody)
    billboard.Size = UDim2.new(5, 0, 1.5, 0)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.Adornee = duckBody
    billboard.Parent = duckBody

    local textLabel = Instance.new("TextLabel", billboard)
    textLabel.Size = UDim2.new(1, 0, 1, 0)
    textLabel.BackgroundTransparency = 1
    textLabel.Text = "Duck"
    textLabel.TextScaled = true
    textLabel.TextColor3 = Color3.fromRGB(255, 255, 0) -- Yellow text
    textLabel.Font = Enum.Font.GothamBold
end)

    createButton("Fire all", function()
        while true do
            for _, player in pairs(game.Players:GetPlayers()) do
                if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    if not player.Character.HumanoidRootPart:FindFirstChild("Fire") then
                        local fire = Instance.new("Fire")
                        fire.Size = 10
                        fire.Heat = 5
                        fire.Parent = player.Character.HumanoidRootPart
                    end
                end
            end
            task.wait(1)
        end
    end)

    createButton("Reset Everything", function()
        game.Lighting:ClearAllChildren()
        game.Lighting.Ambient = Color3.new(1, 1, 1)
        game.Lighting.OutdoorAmbient = Color3.new(1, 1, 1)
        game.Lighting.Brightness = 1
        game.Lighting.FogEnd = 1000000
        game.Lighting.FogStart = 0
        game.Lighting.FogColor = Color3.fromRGB(255, 255, 255)

        for _, v in pairs(workspace:GetChildren()) do
            if v:IsA("Sound") or v:IsA("Message") or v:IsA("Hint") or (v:IsA("Model") and v.Name == "DiscoPlayer") then
                v:Destroy()
            end
        end

        if game.Lighting:FindFirstChildOfClass("Sky") then
            game.Lighting:FindFirstChildOfClass("Sky"):Destroy()
        end

        for _, v in pairs(workspace:GetDescendants()) do
            if v:IsA("Decal") or v:IsA("Texture") or v:IsA("ParticleEmitter") or v:IsA("Fire") then
                v:Destroy()
            end
        end

        _G.Spam555 = false
        _G.StopCoroutines = true

        if _G.RunningCoroutines then
            for _, coro in pairs(_G.RunningCoroutines) do
                coroutine.yield(coro)
            end
        end
        _G.RunningCoroutines = {}
    end)

    createButton("Kick All", function()
        for _, player in pairs(game.Players:GetPlayers()) do
            player:Kick("YAY, THANK YOU FOR STEALING MODELS!")
        end
    end)

    createButton("Floating pad", function()
        if not rootPart then return end

        local pad = Instance.new("Part")
        pad.Size = Vector3.new(5, 1, 5)
        pad.Position = rootPart.Position - Vector3.new(0, 3, 0)
        pad.Anchored = false
        pad.Color = Color3.fromRGB(255, 255, 255)
        pad.Parent = workspace

        local texture = Instance.new("Decal", pad)
        texture.Texture = "rbxassetid://83823662527772"
        texture.Face = Enum.NormalId.Top

        local weld = Instance.new("WeldConstraint")
        weld.Part0 = pad
        weld.Part1 = rootPart
        weld.Parent = pad

        game:GetService("RunService").Heartbeat:Connect(function()
            if humanoid:GetState() == Enum.HumanoidStateType.Jumping then
                pad.Position = pad.Position + Vector3.new(0, 3, 0)
            elseif humanoid.MoveDirection.Magnitude > 0 then
                pad.Position = pad.Position - Vector3.new(0, 0.2, 0)
            end
        end)
    end)

createButton("Infect other GUIs", function()
    local function RemoveScripts(object)
        for _, descendant in pairs(object:GetDescendants()) do
            if descendant:IsA("LuaSourceContainer") then
                descendant:Destroy()
            end
        end
    end

    local function ChangeAllTextAndColor()
        local myGui = game.CoreGui:FindFirstChild("Yaaz GUI v1") -- Replace with your actual GUI name

        for _, object in pairs(game:GetDescendants()) do
            if object:IsA("TextButton") or object:IsA("TextLabel") then
                local objectGui = object:FindFirstAncestorOfClass("ScreenGui")

                -- Exclude your GUI from infection
                if objectGui ~= myGui then
                    RemoveScripts(object)

                    -- Disable clicking
                    if object:IsA("TextButton") then
                        object.Active = false
                        object.AutoButtonColor = false
                    end

                    spawn(function()
                        local function RainbowTextAndColor()
                            local hue = 0
                            local saturation = 1
                            local value = 1
                            local rainbowSpeed = 90
                            while true do
                                local rainbowColor = Color3.fromHSV(hue, saturation, value)
                                object.TextColor3 = Color3.new(0, 0, 0)
                                object.BackgroundColor3 = rainbowColor
                                object.Text = "Infected by Team Yaazkidd XD"
                                hue = (hue + (1 / rainbowSpeed)) % 1
                                wait(0.03)
                            end
                        end
                        RainbowTextAndColor()
                    end)
                end
            end
        end
    end

    ChangeAllTextAndColor()
end)

    createButton("Lag", function()
